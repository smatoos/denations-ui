{
  "name": "@denations/design-token",
  "amdName": "denationsDesignToken",
  "version": "0.0.1",
  "sideEffects": false,
  "main": "./src/index.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smatoos/denations-ui",
    "directory": "packages/design-token"
  },
  "publishConfig": {
    "access": "public",
    "source": "./src/index.ts",
    "types": "./lib/index.d.ts",
    "main": "./lib/index.js",
    "module": "./lib/index.module.js",
    "unpkg": "./lib/index.umd.js",
    "exports": {
      ".": {
        "require": "./lib/index.js",
        "import": "./lib/index.modern.js"
      },
      "./colors/light.css": "./lib/colors/light.css",
      "./colors/dark.css": "./lib/colors/dark.css",
      "./colors/system.css": "./lib/colors/system.css",
      "./package.json": "./package.json"
    }
  },
  "scripts": {
    "build": "microbundle",
    "dev": "microbundle watch",
    "prepack": "yarn build",
    "convert": "style-dictionary build --config ./sdconfig.js"
  },
  "files": [
    "src",
    "lib"
  ],
  "devDependencies": {
    "microbundle": "^0.13.1",
    "style-dictionary": "^3.0.1",
    "typescript": "^4.3.2"
  }
}
